        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LoginPage"
        mc:Ignorable="d"
        Title="UserStatistics" Height="276" Width="300">
    <Grid Margin="0,0,0,4">
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="224" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="272" TextChanged="textBox_TextChanged"/>
    </Grid>
</Window>
